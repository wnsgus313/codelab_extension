{
  "name": "codelab",
  "displayName": "codelab",
  "description": "Assist in programming practice classes",
  "version": "0.0.1",
  "keywords": [
    "class",
    "asist",
    "practice",
    "programming",
    "codelab"
  ],
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.downloadFile",
    "onCommand:extension.fetchProblem",
    "onCommand:extension.uploadProblem",
    "onCommand:extension.submitCode",
    "onCommand:extension.codelabLogin"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.downloadFile",
        "title": "Download File"
      },
      {
        "command": "extension.fetchProblem",
        "title": "Fetch Problem"
      },
      {
        "command": "extension.uploadProblem",
        "title": "Upload Problem"
      },
      {
        "command": "extension.submitCode",
        "title": "Submit Code"
      },
      {
        "command": "extension.codelabLogin",
        "title": "Codelab Login"
      }
    ],
    "configuration": {
      "title": "Download File Configuration",
      "properties": {
        "download-file.defaultFolder": {
          "type": "string",
          "default": "",
          "description": "Default folder where file should be saved."
        },
        "url.uploadCode": {
          "type": "string",
          "default": "http://203.245.41.143:5000/api/v1/problems/",
          "description": "Code upload base url"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "axios": "^0.26.1"
  }
}
